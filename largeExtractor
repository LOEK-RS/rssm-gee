

var aois = ee.FeatureCollection("projects/ee-ludwigm6/assets/peat/natura2000_moorwaelder_develsample")


var startYear = 2015
var endYear = 2018
var startMonth = 6
var endMonth = 8




var extracted = aois.map(function(feature){
  
  
  var l9 = ee.ImageCollection('LANDSAT/LC09/C02/T1_L2')
  .filterBounds(feature)
  .filter(ee.Filter.calendarRange(startYear, endYear, 'year'))
  .filter(ee.Filter.calendarRange(startMonth, endMonth, 'month'))
  .filter(ee.Filter.lt('CLOUD_COVER', 80))
  .select(['SR_B4', 'SR_B5', 'QA_PIXEL', 'QA_RADSAT'])
  .toBands()

  
  return ee.Feature(feature.geometry(), l9.reduceRegion({
    reducer: ee.Reducer.median(),
    geometry: feature.geometry(),
    scale: 30
  }))
  
  
})


print(extracted)





Map.addLayer(aois, {}, "moore")



